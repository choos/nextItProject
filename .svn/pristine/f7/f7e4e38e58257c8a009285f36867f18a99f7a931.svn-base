padding: 0;
}

ul,ol {
    list-style: none;
}

a {
    text-decoration: none;
    color: inherit;
}


/* con4----------------------------------------------------------- */
#con4 {
    width:100%; 
    height:200px;
    position: relative;
/*     background: #ffd500; */
    background-size: cover;
    box-sizing: border-box;
}
#con4 > .con4 {
/*     width: 1400px;
    height: 1050px; */
    width: 1000px;
    height: 750px;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    transition: 1s;
    opacity: 1;
}
#con4 > .con4::after {
    display: block;
    content: "";
    clear: both;
}
#con4 > .con4 > .flip-container {
    width: 250px;
    height: 250px;
    border: 1px solid blue;
    /* overflow: hidden; */
    float: left;
    /* perspective: 1000px; */
}
#con4 > .con4 > .flip-container > .flipper {
    transition: 0.5s;
    transform-style: preserve-3d;
    position: relative;
    cursor: pointer;
}
.flip-container:hover > .flipper {
    transform: rotateY(90deg);
}
#con4 > .con4 >.flip-container > .flipper > .front {
    width: 250px;
    height: 250px;
    position: absolute;
    backface-visibility: hidden;
    z-index: 2;
    transform-origin: 100% 0%;
    transform: rotateY(0deg);
    transition: 0.5s;
    border: 1px solid #000;
    box-sizing: border-box;
}
#con4 > .con4 >.flip-container:hover > .flipper > .front {
    transform: rotateY(90deg);
}
#con4 > .con4 >.flip-container > .flipper > .back {
    width: 250px;
    height: 250px;
    left: -175px;
    position: absolute;
    backface-visibility: hidden;
    transform: rotateY(-90deg);
}
#con4 > .con4 > .flip-container > .flipper > .back > .con4-back-text {
    position: absolute;
    top: 8%;
    left: 10%;
    font-size: 15px;
}
#con4 > .con4 > .flip-container > .flipper > .back > .con4-back-more {
    position: absolute;
    bottom: 5%;
    right: 5%;
    font-size: 15px;
    transition: 0.5s;
}
#con4 > .con4 > .flip-container > .flipper > .back > .con4-back-more:hover {
    color: rgba(0, 0, 0, 0.5);
}
#con4 > .con4 >.flip-container > .flipper > .con4front-img-1{
/*     background: url(../img4/con4img-1.png)center center no-repeat; */
    background-size: contain;
}
#con4 > .con4 > .flip-container > .flipper > .con4back-img-1{
    background: rgba(0, 0, 0, 0.5);
    box-sizing: border-box;
}
#con4 > .con4 > .flip-container > .flipper > .con4back-img-1 > .con4-back-text {
    color: #fff;
}
#con4 > .con4 > .flip-container > .flipper > .con4back-img-1 > .con4-back-more {
    color: #fff;
}
#con4 > .con4 > .flip-container > .flipper > .con4back-img-1 > .con4-back-more:hover {
    color: rgba(255, 255, 255, 0.5);
}
