package com.zero.shoppingCart.vo;

import org.apache.commons.lang3.builder.ToStringBuilder;

public class CartVO {
	
	//only cartVO
	private String sCCode;
	private String sCSCode;
	private int sCQuantity;
	
	//SB VO 판매 품목 정보 가져오기
	private String sTitle;
	private String sPrice;
	private String sMainIMG;
	
	public String getsCCode() {
		return sCCode;
	}

	public void setsCCode(String sCCode) {
		this.sCCode = sCCode;
	}

	public String getsCSCode() {
		return sCSCode;
	}

	public void setsCSCode(String sCSCode) {
		this.sCSCode = sCSCode;
	}

	public int getsCQuantity() {
		return sCQuantity;
	}

	public void setsCQuantity(int sCQuantity) {
		this.sCQuantity = sCQuantity;
	}

	public String getsTitle() {
		return sTitle;
	}

	public void setsTitle(String sTitle) {
		this.sTitle = sTitle;
	}

	public String getsPrice() {
		return sPrice;
	}

	public void setsPrice(String sPrice) {
		this.sPrice = sPrice;
	}

	public String getsMainIMG() {
		return sMainIMG;
	}

	public void setsMainIMG(String sMainIMG) {
		this.sMainIMG = sMainIMG;
	}

	@Override
	public String toString() {
		return ToStringBuilder.reflectionToString(this);
	}
	
	
	
}
