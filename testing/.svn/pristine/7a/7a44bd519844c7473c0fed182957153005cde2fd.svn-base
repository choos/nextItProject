package com.zero.FAQ.vo;

import org.apache.commons.lang3.builder.ToStringBuilder;

public class FAQVO {
	private  int faNum;              /*PK 질문 번호*/
	private  String faTitle;         /*질문 제목*/
	private  String faCategory;      /*질문 분류*/
	private  String faContent;       /*질문 내용*/
	private  String faDelYn;         /*질답 삭제 여부*/
	private  String faMId;           /*FAQ 관리자 아이디*/
	
	@Override
	public String toString() {
		return ToStringBuilder. reflectionToString(this);
	}
	
	public int getFaNum() {
		return faNum;
	}
	public String getFaTitle() {
		return faTitle;
	}
	public String getFaCategory() {
		return faCategory;
	}
	public String getFaContent() {
		return faContent;
	}
	public String getFaDelYn() {
		return faDelYn;
	}
	public String getFaMId() {
		return faMId;
	}
	public void setFaNum(int faNum) {
		this.faNum = faNum;
	}
	public void setFaTitle(String faTitle) {
		this.faTitle = faTitle;
	}
	public void setFaCategory(String faCategory) {
		this.faCategory = faCategory;
	}
	public void setFaContent(String faContent) {
		this.faContent = faContent;
	}
	public void setFaDelYn(String faDelYn) {
		this.faDelYn = faDelYn;
	}
	public void setFaMId(String faMId) {
		this.faMId = faMId;
	}
	
}
